{
  "name": "braunstetter/data-grid-bundle",
  "type": "symfony-bundle",
  "description": "Create data grids and reuse it everywhere.",
  "license": "MIT",
  "authors": [
    {
      "name": "Slowwie",
      "email": "michaelbrauner82@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "symfony/framework-bundle": "^4.4|^5.0",
    "symfony/twig-bundle": "5.3.*",
    "symfony/options-resolver" : "5.3.*",
    "symfony/property-access": "^5.3",
    "pagerfanta/twig": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "Braunstetter\\DataGridBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Braunstetter\\DataGridBundle\\Tests\\": "Tests/"
    }
  },
  "require-dev": {
    "symfony/test-pack": "^1.0",
    "matthiasnoback/symfony-dependency-injection-test": "^4.2",
    "nyholm/symfony-bundle-test": "^1.8"
  }
}
